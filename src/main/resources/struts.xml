<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
  	
    <package name="default" namespace="/" extends="struts-default">
        <result-types>
            <result-type name="json" class="org.apache.struts2.json.JSONResult"/>
        </result-types>    
        <action name="displayStudentList" class="com.v2crm.web.actions.StudentsAction" 
                  method="displayStudentList">
		   <result name="success">/displayTag.jsp</result>
		</action>
         <action name="getLeadsJson" class="com.v2crm.web.actions.LeadAction">
            <result type="json" />
         </action>
         
         <action name="listLeads" class="com.v2crm.web.actions.LeadAction">
                <result type="json">pages/leadsTable.jsp</result>
          </action>        
       
    </package>

</struts>